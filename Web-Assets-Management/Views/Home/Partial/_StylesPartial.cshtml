<style>

/* Privacy Policy Page*/

article {
  width: 70%;
  border: 1px solid black;
  background-color: #cccccc;
  padding: 15px;
  margin: 15%;
  margin-top: 5%;
  margin-bottom: 5%;
  font-size: 22px
}

p {
  font-size: 20px;
  line-height: 30px;
  text-align: left;
}

    /*      ---      */

* {
    box-sizing: border-box;
}

body {
  margin: 0;
  font-family: Arial, Helvetica, sans-serif;

}

/* Header Area */

.header {
  overflow: hidden;
  background-color: bisque;
  padding: 20px 10px;
}

.header a {
  float: left;
  color: black;
  text-align: center;
  padding: 12px;
  text-decoration: none;
  font-size: 18px;
  line-height: 25px;
  border-radius: 4px;
}

.header a.logo {
  font-size: 25px;
  font-weight: bold;
}

.header a:hover {
  background-color: #ddd;
  color: black;
}

.header a.active {
  background-color: dodgerblue;
  color: white;
}

.header-right {
  float: right;
}

/* Footer Area */

 .footer {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    background-color: bisque;
    color: black;
    text-align: center;
}

.footer p {
    float: left;
    color: black;
    text-align: center;
    padding: 12px;
    font-size: 15px;
}

/* Content Area */

.center-div {
    padding: 20px;
    margin-bottom: 20px;
    text-align: center;
    font-size: 18px;
}

/* Table Area */

.table-wrapper{
    margin: 10px 70px 70px;
    box-shadow: 0px 35px 50px rgba( 0, 0, 0, 0.2 );

}

.fl-table {
    border-radius: 5px;
    font-size: 12px;
    font-weight: normal;
    border: none;
    border-collapse: collapse;
    width: 100%;
    max-width: 100%;
    white-space: nowrap;
    background-color: white;
}

.fl-table td, .fl-table th {
    text-align: center;
    padding: 8px;
}

.fl-table td {
    border-right: 1px solid #f8f8f8;
    font-size: 12px;
}

.fl-table thead th {
    color: black;
    background: bisque;
}


.fl-table thead th:nth-child(odd) {
    color: #ffffff;
    background: #324960;
}

.fl-table tr:nth-child(even) {
    background: #F8F8F8;
}

/* Responsive */

.media (max-width: 767px) {
    .fl-table {
        display: block;
        width: 100%;
    }
    .table-wrapper:before{
        content: "Scroll horizontally >";
        display: block;
        text-align: right;
        font-size: 11px;
        color: white;
        padding: 0 0 10px;
    }
    .fl-table thead, .fl-table tbody, .fl-table thead th {
        display: block;
    }
    .fl-table thead th:last-child{
        border-bottom: none;
    }
    .fl-table thead {
        float: left;
    }
    .fl-table tbody {
        width: auto;
        position: relative;
        overflow-x: auto;
    }
    .fl-table td, .fl-table th {
        padding: 20px .625em .625em .625em;
        height: 60px;
        vertical-align: middle;
        box-sizing: border-box;
        overflow-x: hidden;
        overflow-y: auto;
        width: 120px;
        font-size: 13px;
        text-overflow: ellipsis;
    }
    .fl-table thead th {
        text-align: left;
        border-bottom: 1px solid #f7f7f9;
    }
    .fl-table tbody tr {
        display: table-cell;
    }
    .fl-table tbody tr:nth-child(odd) {
        background: none;
    }
    .fl-table tr:nth-child(even) {
        background: transparent;
    }
    .fl-table tr td:nth-child(odd) {
        background: #F8F8F8;
        border-right: 1px solid #E6E4E4;
    }
    .fl-table tr td:nth-child(even) {
        border-right: 1px solid #E6E4E4;
    }
    .fl-table tbody td {
        display: block;
        text-align: center;
    }

}
    .fl-table tbody tr button {
        position: relative;
        border-radius: 15px 15px 15px 15px;
        width: 100px;
        height: 25px;
        border: none;
        outline: none;
        cursor: pointer;
        background-color: #324960;
        color: red;
        transition: background-color 0.3s ease;
    }

        .fl-table tbody tr button:hover {
            background-color: bisque;
        }

/* List Database */

.content-get-data {
    display:flex;
    margin:10px;
    position:absolute;

}

/* Ul and Li Area */

.sidenav-static {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.sidenav {
  height: 100%;
  width: 0;
  position: fixed;
  z-index: 1;
  top: 0;
  left: 0;
  background-color: bisque;
  overflow-x: hidden;
  transition: 0.5s;
  padding-top: 60px;
  text-align:center;
}

.sidenav a {
  padding: 8px 8px 8px 32px;
  text-decoration: none;
  font-size: 25px;
  color: white;
  display: block;
  transition: 0.3s;

}

.sidenav a:hover{
  color: black;
}

.sidenav .closebtn {
  position: absolute;
  top: 0;
  right: 25px;
  font-size: 36px;
  margin-left: 50px;
}

/* Form Partial */

hr {
  display: block;
  margin-top: 0.5em;
  margin-bottom: 0.5em;
  margin-left: auto;
  margin-right: auto;
  border-style: inset;
  border-width: 1px;
  background-color:bisque;
  padding: 10px;
}

.form-action {
    margin:0;
    padding:0;
    font-family:sans-serif;
    display:block;
}

.form-action select {
    position: relative;
    display: inline-block;
    font-size: 20px;
    box-sizing: border-box;
    transition: .5s;
    width: 340px;
    height: 50px;
    border: none;
    outline: none;
    padding: 0 25px;
    border-radius: 25px 25px 25px 25px;
    border: 2px solid black;
    margin: 3px;
}

.form-action h1 {
    margin: 0 0 10px;
    padding:0;
    color: bisque;
    font-size:24px;
}

.box {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

.form-action input {
    position: relative;
    display: inline-block;
    font-size: 20px;
    box-sizing: border-box;
    transition: .5s;
    width: 340px;
    height: 50px;
    border: none;
    outline: none;
    padding: 0 25px;
    border-radius: 25px 25px 25px 25px;
    border: 2px solid black;
    margin: 3px;
}

.form-action label.checkbox-label {
    display: block;
    font-size: 20px;
    color: #000;
    margin-bottom: 10px;
}

.form-action input[type="checkbox"] {
    position: relative;
    display: inline-block;
    width: 20px;
    height: 20px;
    margin-right: 10px;
}

.form-action input[type="checkbox"]::before {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border: 2px solid black;
    border-radius: 4px;
    background-color: #fff;
}

.form-action input[type="checkbox"]:checked::before {
    background-color: bisque;
}

.form-action input[type="checkbox"]:checked::after {
    content: "\2713";
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    font-size: 16px;
    color: #000;
}


.form-action button {
    position: relative;
    border-radius: 25px 25px 25px 25px;
    width: 150px;
    height: 50px;
    border: none;
    outline: none;
    cursor: pointer;
    background-color: #324960;
    color:red;
    transition: background-color 0.3s ease;
}

.form-action button:hover {
    background-color:bisque;
}

.popup-container {
    display: inline-flex;
}

.button {
    position: relative;
    border-radius: 15px;
    width: 100px;
    height: 25px;
    border: none;
    outline: none;
    cursor: pointer;
    background-color: #324960;
    color: red;
    transition: background-color 0.3s ease;
}

.popup {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.5);
    z-index: 10;
    opacity: 0;
    visibility: hidden;
    transition: 400ms all;
}

.popup > label {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    cursor: pointer;
}

.popup .inner {
    position: absolute;
    top: 100%;
    left: 50%;
    transform: translate(-50%, -50%);
    background: #fff;
    min-width: 300px;
    box-sizing: border-box;
    transition: 400ms all;
    z-index: 10;
    max-height: 100%;
    overflow: auto;
}

.popup .inner .title {
    height: 40px;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 15px;
    position: sticky;
    top: 0;
    background: #fff;
}

.popup .inner .title h4 {
    font-size: 15px;
    font-weight: 500;
}

.popup .inner .title label {
    font-size: 14px;
    color: #999;
    cursor: pointer;
}

.popup .inner .title label:hover {
    color: #222;
}

.popup .inner .content {
    padding: 5px 15px 15px;
}

.popup .inner .content input,
.popup .inner .content select {
    width: 100%;
    box-sizing: border-box;
    border: none;
    border-bottom: 1px solid #ddd;
    padding-bottom: 10px;
    font-size: 12px;
}

.popup .inner .content input:focus,
.popup .inner .content select:focus {
    outline: 0;
    border-color: blue;
}

.popup .inner .content button {
    width: 100%;
    height: 30px;
    border-radius: 3px;
    border: none;
    background: blue;
    color: #fff;
    font-size: 13px;
    cursor: pointer;
}

.popup .inner .content button:hover {
    background-color: bisque;
}

.popup .inner p {
    font-size: 13px;
    line-height: 130%;
    margin-bottom: 15px;
}

.popup .inner p:last-child {
    margin-bottom: 0;
}

.popup .inner .close-btn {
    margin-top: 15px;
    padding-top: 15px;
    border-top: 1px solid #ddd;
}

.popup .inner .close-btn label {
    background: rgba(0, 0, 0, 0.05);
    display: block;
    line-height: 30px;
    text-align: center;
    font-size: 13px;
    color: #444;
    border-radius: 3px;
    cursor: pointer;
}

.popup .inner .close-btn label:hover {
    background: rgba(0, 0, 0, 0.1);
}

.popup-container > input {
    position: absolute;
    left: -9999px;
    opacity: 0;
}

.popup-container > input:checked + .popup {
    opacity: 1;
    visibility: visible;
}

.popup-container > input:checked + .popup .inner {
    top: 50%;
}


</style>